{
  "name": "Archive",
  "query": {
    "folder": "efforts/archive"
  },
  "view": {
    "type": "table",
    "columns": [
      {"property": "file.link", "label": "Effort"},
      {"property": "area", "label": "Area"},
      {"property": "file.mtime", "label": "Completed"}
    ]
  },
  "sort": [
    {"property": "file.mtime", "direction": "desc"}
  ],
  "limit": 10
}
